                        "in": "path",
                        "schema": {
                            "type": "string",
                            "example": "a931001e-56ff-4752-b3db-f7ed3544c385"
                        },
                        "required": true
                    }
                ],
                "responses": {
                    "200": {
                        "description": "Successful operation",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "example": {
                                        "uuid": "a931001e-56ff-4752-b3db-f7ed3544c385",
                                        "email": "test@test.pl"
                                    }
                                }
                            }
                        }
                    },
                    "400": {
                        "description": "Validation error",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "errors": {
                                            "type": "object",
                                            "properties": {
                                                "uuid": {
                                                    "type": "array",
                                                    "example": [
                                                        "INVALID_UUID"
                                                    ]
                                                }
                                            }
                                        }
                                    }
                                }
                            }
                        }
                    },
                    "404": {
                        "description": "Not found",
                        "content": {
                            "application/json": {
                                "schema": {
                                    "type": "object",
                                    "properties": {
                                        "type": {
                                            "type": "string",
                                            "example": "USER_NOT_FOUND"
                                        }
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
